{
  "name": "vibewell",
  "version": "1.0.0",
  "description": "VibeWell Beauty & Wellness Platform",
  "private": true,
  "scripts": {
    "dev:web": "turbo run dev --filter=@vibewell/web",
    "dev:mobile": "turbo run dev --filter=@vibewell/mobile",
    "build": "turbo run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "storybook": "turbo run storybook --filter=@vibewell/ui-web",
    "infra:up": "docker-compose up -d",
    "chromatic": "pnpm --filter=@vibewell/ui-web run chromatic"
  },
  "keywords": [
    "beauty",
    "wellness",
    "booking",
    "e-commerce",
    "saas"
  ],
  "author": "VibeWell Development Team",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@playwright/test": "^1.52.0",
    "@storybook/builder-webpack5": "^8.6.14",
    "@storybook/react-webpack5": "^7.6.20",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.50",
    "@types/react": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "css-loader": "^6.11.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "null-loader": "^4.0.1",
    "postcss-loader": "^7.3.4",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.3.4",
    "turbo": "2.5.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@supabase/supabase-js": "^2.49.8",
    "@types/react-datepicker": "^7.0.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.12.2",
    "react-datepicker": "^8.3.0",
    "workbox-window": "^7.3.0"
  }
}
